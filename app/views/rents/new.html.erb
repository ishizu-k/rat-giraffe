<h1>物件登録</h1>

<%= form_for(@rent) do |form| %>
  <div class="field">
    <%= form.label :物件名 %><br>
    <%= form.text_field :property_name %>
  </div>
  <div class="field">
    <%= form.label :賃料 %><br>
    <%= form.text_field :fee %>円
  </div>
  <div class="field">
    <%= form.label :住所 %><br>
    <%= form.text_field :address %>
  </div>
  <div class="field">
    <%= form.label :築年数 %><br>
    <%= form.text_field :age %>年
  </div>
  <div class="field">
    <%= form.label :備考 %><br>
    <%= form.text_area :note %>
  </div>
  <hr>

  <h3>最寄り駅1</h3>

<!-- ここに最寄り駅の入力フォーム -->

  <h3>最寄り駅2</h3>

<!-- ここに最寄り駅の入力フォーム -->

  <div class="field">
  <%= form.submit "登録する" %>
  </div>
<% end %>

<br>
<p>戻るリンク</p>
